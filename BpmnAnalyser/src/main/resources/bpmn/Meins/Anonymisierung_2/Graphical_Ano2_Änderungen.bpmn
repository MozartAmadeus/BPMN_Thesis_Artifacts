<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" id="Definitions_36" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="Process_36" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:task id="Task_1" name="Jedes bearbeitete B wird einer C zugeordnet">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_2" name="Jeder D ist einer C zugeordnet">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>

    <bpmn:exclusiveGateway id="XOR_Gateway_1">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing> <!-- C ist neu -->
      <bpmn:outgoing>Flow_5</bpmn:outgoing> <!-- C existiert bereits -->
    </bpmn:exclusiveGateway>

    <bpmn:task id="Task_3" name="C-ID im Window-Title anzeigen">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_4" name="Verschmelzen der Cs, Benutzer wird informiert">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>

    <bpmn:exclusiveGateway id="XOR_Join_1">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:endEvent id="EndEvent_1">
      <bpmn:incoming>Flow_7</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_1"/>
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_1" targetRef="Task_2"/>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_2" targetRef="XOR_Gateway_1"/>
    <bpmn:sequenceFlow id="Flow_4" sourceRef="XOR_Gateway_1" targetRef="Task_3"/>
    <bpmn:sequenceFlow id="Flow_5" sourceRef="XOR_Gateway_1" targetRef="Task_4"/>
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_3" targetRef="XOR_Join_1"/>
    <bpmn:sequenceFlow id="Flow_6b" sourceRef="Task_4" targetRef="XOR_Join_1"/>
    <bpmn:sequenceFlow id="Flow_7" sourceRef="XOR_Join_1" targetRef="EndEvent_1"/>
  </bpmn:process>
<bpmndi:BPMNDiagram xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"><bpmndi:BPMNPlane bpmnElement="Process_36"><bpmndi:BPMNShape bpmnElement="StartEvent_1"><dc:Bounds xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" x="150" y="250" width="80" height="80"/></bpmndi:BPMNShape><bpmndi:BPMNShape bpmnElement="Task_1"><dc:Bounds xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" x="300" y="250" width="100" height="80"/></bpmndi:BPMNShape><bpmndi:BPMNShape bpmnElement="Task_2"><dc:Bounds xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" x="450" y="250" width="100" height="80"/></bpmndi:BPMNShape><bpmndi:BPMNShape bpmnElement="XOR_Gateway_1"><dc:Bounds xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" x="600" y="250" width="80" height="80"/></bpmndi:BPMNShape><bpmndi:BPMNShape bpmnElement="Task_3"><dc:Bounds xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" x="750" y="125" width="100" height="80"/></bpmndi:BPMNShape><bpmndi:BPMNShape bpmnElement="Task_4"><dc:Bounds xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" x="750" y="375" width="100" height="80"/></bpmndi:BPMNShape><bpmndi:BPMNShape bpmnElement="XOR_Join_1"><dc:Bounds xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" x="900" y="250" width="80" height="80"/></bpmndi:BPMNShape><bpmndi:BPMNShape bpmnElement="EndEvent_1"><dc:Bounds xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" x="1050" y="250" width="80" height="80"/></bpmndi:BPMNShape><bpmndi:BPMNEdge bpmnElement="Flow_1"><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="230" y="290"/><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="300" y="290"/></bpmndi:BPMNEdge><bpmndi:BPMNEdge bpmnElement="Flow_2"><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="400" y="290"/><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="450" y="290"/></bpmndi:BPMNEdge><bpmndi:BPMNEdge bpmnElement="Flow_3"><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="550" y="290"/><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="600" y="290"/></bpmndi:BPMNEdge><bpmndi:BPMNEdge bpmnElement="Flow_4"><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="680" y="290"/><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="750" y="165"/></bpmndi:BPMNEdge><bpmndi:BPMNEdge bpmnElement="Flow_5"><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="680" y="290"/><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="750" y="415"/></bpmndi:BPMNEdge><bpmndi:BPMNEdge bpmnElement="Flow_6"><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="850" y="165"/><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="900" y="290"/></bpmndi:BPMNEdge><bpmndi:BPMNEdge bpmnElement="Flow_6b"><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="850" y="415"/><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="900" y="290"/></bpmndi:BPMNEdge><bpmndi:BPMNEdge bpmnElement="Flow_7"><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="980" y="290"/><di:waypoint xmlns:di="http://www.omg.org/spec/DD/20100524/DI" x="1050" y="290"/></bpmndi:BPMNEdge></bpmndi:BPMNPlane></bpmndi:BPMNDiagram></bpmn:definitions>