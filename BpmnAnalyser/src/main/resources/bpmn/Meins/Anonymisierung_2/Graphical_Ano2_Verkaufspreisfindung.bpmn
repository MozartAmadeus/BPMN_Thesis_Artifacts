      <bpmn:outgoing>Flow_14</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_14" name="H lt. U verdeckt? Dann in V einrechnen (2 Nachkommastellen)">
      <bpmn:incoming>Flow_14</bpmn:incoming>
      <bpmn:outgoing>Flow_15</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_15" name="W für alle DS ermitteln, X ggf. bearbeiten">
